          version='${PROJECT_VERSION}',
          package_dir={ '': '${CMAKE_CURRENT_SOURCE_DIR}' },
          ext_modules=[Extension('${PROJECT_NAME}', ['${CMAKE_CURRENT_SOURCE_DIR}/${SOURCE_FILES}'])])

